{
    "buildCommand": "NODE_ENV=production expo export -p web",
    "outputDirectory": "dist",
    "devCommand": "expo",
    "cleanUrls": true,
    "framework": null,
    "redirects": [
      {
        "source": "/",
        "destination": "/app",
        "permanent": false
      }
    ],
    "rewrites": [
      {
        "source": "/app/:path*",
        "destination": "/"
      }
    ]
  }
  